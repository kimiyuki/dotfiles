{
    "memo": "linked from ~/.config/peco/config.json",
    "CustomFilter": {
        "grepV": {
            "Cmd" : "grepv.pl",
            "Args" : [ "$QUERY" ],
            "BufferThreshold" : 100
        },
        "Migemo" : {
            "Cmd" : "migemogrep",
            "BufferThreshold" : 20
        }
    },
    "Keymap": {
        "C-u": "peco.ScrollPageUp",
        "C-d": "peco.ScrollPageDown",
        "C-x,C-c": "peco.Cancel"
    }
}
